{
    "name": "accordous/fluxous-client",
    "description": "Client for Fluxous API",
    "type": "library",
    "authors": [
        {
            "name": "Accordous",
            "email": "contato@accordous.com.br"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
              "FluxousClientServiceProvider"
            ],
            "aliases": {
              "FluxousClient" : "Accordous\\FluxousClient\\Facades\\FluxousClient"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Accordous\\FluxousClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Accordous\\FluxousClient\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/support": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.5|^7.3",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    }
}
